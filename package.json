  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.2",
    "@vercel/ncc": "^0.38.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "@actions/core": "^1.11.1",
